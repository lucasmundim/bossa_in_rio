$(document).ready(function () {
  $('.alert-messages').html('<div class="alert-message info"><%= content_tag :div, flash[:notice] %></div>');

  $('#<%= dom_id(@photo, :modal) %>').modal('hide');

  $('#<%= dom_id(@photo) %> img').attr('src', '<%= @photo.image.send(@photo.thumb_version || "thumb").url %>');
});